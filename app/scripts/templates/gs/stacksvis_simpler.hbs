<div>
    {{#each items_by_tumor_type}}
        <div class="accordion-heading">
            <a class="accordion-toggle" data-toggle="collapse" href="#accordion-collapse-{{tumor_type}}-{{id}}">{{tumor_type}} <i class="icon-chevron-down"></i></a>
        </div>
        <div id="accordion-collapse-{{tumor_type}}-{{id}}" class="accordion-body collapse {{#unless @index}}in{{/unless}}">
            <div class="accordion-inner">
                <table class="table table-striped table-bordered table-hover table-condensed">
                    <thead>
                    <tr>
                        <td>Gene</td>
                        <td>Cytoband</td>
                        <td>Q-Value</td>
                        <td>Type</td>
                        <td>Samples (#/%NA)</td>
                        <td>Deletions (#/%)</td>
                        <td>Gains (#/%)</td>
                        <td>Mutations (#/%)</td>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each items}}
                    <tr>
                        <td>{{gene}}</td>
                        <td>{{cytoband}}</td>
                        <td>{{q_value}}
                        <td>{{type}}</td>
                        <td>{{samples.numberOf}} / {{samples.percentOf}}</td>
                        <td>{{deletions.numberOf}} / {{deletions.percentOf}}</td>
                        <td>{{gains.numberOf}} / {{gains.percentOf}}</td>
                        <td>{{mutations.numberOf}} / {{mutations.percentOf}}</td>
                    </tr>
                    {{/each}}
                    </tbody>
                </table>
                <div class="heatmap-{{tumor_type}}">
                    <div class="stacksvis-heatmap"></div>
                </div>
            </div>
        </div>
    {{/each}}
    </ul>
</div>